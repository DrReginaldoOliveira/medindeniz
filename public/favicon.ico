<svg width="64" height="64" viewBox="0 0 64 64" xmlns="http://www.w3.org/2000/svg">
  <!-- Definição de gradientes -->
  <defs>
    <linearGradient id="goldGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#f6e05e" />
      <stop offset="100%" stop-color="#d69e2e" />
    </linearGradient>
    <linearGradient id="blueGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#3182ce" />
      <stop offset="100%" stop-color="#2B6CB0" />
    </linearGradient>
  </defs>
  
  <!-- Fundo circular -->
  <circle cx="32" cy="32" r="30" fill="white" stroke="#2B6CB0" stroke-width="2" />
  
  <!-- Grupo do ícone (balança + estetoscópio) -->
  <g transform="translate(12, 10) scale(1)">
    <!-- Base da balança -->
    <rect x="10" y="35" width="30" height="4" rx="2" fill="url(#goldGradient)" />
    
    <!-- Pilar da balança -->
    <rect x="23" y="15" width="4" height="20" fill="url(#goldGradient)" />
    
    <!-- Pratos da balança -->
    <ellipse cx="13" cy="15" rx="8" ry="2.5" fill="url(#goldGradient)" opacity="0.9" />
    <ellipse cx="37" cy="15" rx="8" ry="2.5" fill="url(#goldGradient)" opacity="0.9" />
    
    <!-- Linhas de suspensão -->
    <line x1="13" y1="15" x2="23" y2="15" stroke="url(#goldGradient)" stroke-width="2" />
    <line x1="37" y1="15" x2="27" y2="15" stroke="url(#goldGradient)" stroke-width="2" />
    
    <!-- Estetoscópio -->
    <path d="M 25 8 C 20 0 15 5 15 10 C 15 15 20 20 25 20 C 30 20 35 15 35 10 C 35 5 30 0 25 8" 
          stroke="url(#goldGradient)" 
          stroke-width="2.5" 
          fill="none" />
    
    <!-- Extremidades do estetoscópio -->
    <circle cx="15" cy="10" r="2.5" fill="url(#goldGradient)" />
    <circle cx="35" cy="10" r="2.5" fill="url(#goldGradient)" />
  </g>
  
  <!-- Letra M estilizada -->
  <text x="20" y="55" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#2B6CB0">MI</text>
</svg>